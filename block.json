{
	"apiVersion": 2,
	"name": "formello/popper",
	"title": "Pop up",
	"icon": "external",
	"category": "widgets",
	"description": "A great popup block.",
	"textdomain": "popper",
	"supports": {
		"html": false,
		"multiple": false,
		"lock": false,
		"className": false,
		"reusable": false,
		"spacing": {
			"padding": true,
			"margin": true
		},
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true,
			"__experimentalDefaultControls": {
				"color": true,
				"radius": true,
				"style": true,
				"width": true
			}
		}
	},
	"attributes": {
		"lock": {
			"type": "object",
			"default": {
				"move": true,
				"remove": true
			}
		},
		"align": {
			"type": "string",
			"default": "center center"
		},
		"anchor": {
			"type": "string",
			"default": ""
		},
		"animation": {
			"type": "string"
		},
		"backgroundColor": {
			"type": "string"
		},
		"boxShadow": {
			"type": "string",
			"default": "wp-block-popper__shadow-md"
		},
		"closeAnchor": {
			"type": "string"
		},
		"closeButtonAlignment": {
			"type": "string",
			"default": "inside"
		},
		"closeButtonColor": {
			"type": "string"
		},
		"closeButtonSize": {
			"type": "number",
			"default": 20
		},
		"closeButtonStyle": {
			"type": "object"
		},
		"closeOnAnchorClick": {
			"type": "boolean",
			"default": false
		},
		"closeOnClickOutside": {
			"type": "boolean",
			"default": true
		},
		"closeOnFormSubmission": {
			"type": "boolean",
			"default": false
		},
		"devices": {
			"type": "array",
			"default": ["desktop", "tablet", "mobile"]
		},
		"dismissForVisitors": {
			"type": "boolean",
			"default": true
		},
		"dismissPeriod": {
			"type": "number",
			"default": 1
		},
		"gradientBackground": {
			"type": "string"
		},
		"fullPage": {
			"type": "boolean",
			"default": false
		},
		"offset": {
			"type": "number",
			"default": 20
		},
		"openBehaviour": {
			"type": "string",
			"default": "load"
		},
		"overlayColor": {
			"type": "string"
		},
		"showCloseButton": {
			"type": "boolean",
			"default": true
		},
		"pageviews": {
			"type": "number"
		},
		"target": {
			"type": "string",
			"default": ""
		},
		"uuid": {
			"type": "number",
			"default": null
		},
		"waitTime": {
			"type": "number",
			"default": 1
		},
		"width": {
			"type": "number",
			"default": 500
		}
	},
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css",
	"viewScript": "file:./build/frontend.js"
}